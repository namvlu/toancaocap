    "# Thực hành 13: Bài toán lợi nhuận đầu tư\n",
    "from sympy import symbols, integrate, exp\n",
    "t = symbols('t')\n",
    "# Hàm thu nhập\n",
    "F = 62 + 8*t\n",
    "# Lãi suất\n",
    "r = 0.017\n",
    "# Giá trị hiện tại thu nhập năm đầu\n",
    "profit_1 = integrate(70*exp(-r*t), (t, 0, 1))\n",
    "# Giá trị hiện tại từ năm 1 đến 8\n",
    "profit_rest = integrate(F*exp(-r*t), (t, 1, 8))\n",
    "total_profit = profit_1 + profit_rest\n",
    "print(\"Tổng giá trị thu về:\", total_profit.evalf())"
