    "# Thực hành 11: Bài toán Black Friday - Tìm giá tốt cho đợt giảm giá điện thoại iPhone 11 cuối năm\n",
    "from sympy import symbols, diff, solve\n",
    "# Khai báo biến\n",
    "p = symbols('p')\n",
    "# Số lượng bán\n",
    "q = 5000 + 1000*(1500 - p)/100\n",
    "# Hàm lợi nhuận\n",
    "L = q*p - 2000000 - 500*q\n",
    "# Đạo hàm\n",
    "dL = diff(L, p)\n",
    "# Tìm giá trị tối ưu\n",
    "p_opt = solve(dL, p)[0]\n",
    "L_max = L.subs(p, p_opt)\n",
    "print(\"Giá bán tối ưu:\", p_opt)\n",
    "print(\"Lợi nhuận tối đa:\", L_max)"
   ]
