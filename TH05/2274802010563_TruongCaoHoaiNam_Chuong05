   "source": [
    "# Thực hành 12: Bài toán Cung - cầu\n",
    "from sympy import symbols, solve, integrate\n",
    "R = symbols('R')\n",
    "# Hàm cầu và cung\n",
    "Qd = -0.8*R + 150\n",
    "Qs = 5.2*R\n",
    "# Tìm điểm cân bằng\n",
    "R_eq = solve(Qd - Qs, R)[0]\n",
    "Q_eq = Qd.subs(R, R_eq)\n",
    "print(\"Giá cân bằng:\", R_eq)\n",
    "print(\"Số lượng cân bằng:\", Q_eq)\n",
    "# Consumer surplus\n",
    "CS = integrate(Qd, (R, 0, R_eq)) - R_eq * Q_eq\n",
    "# Producer surplus\n",
    "PS = R_eq * Q_eq - integrate(Qs, (R, 0, R_eq))\n",
    "print(\"Tổng số tiền người mua phải trả dư:\", CS)\n",
    "print(\"Tổng số tiền mà sản phẩm thu dư được:\", PS)"
   ]
  },
